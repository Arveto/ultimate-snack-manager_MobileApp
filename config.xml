<?xml version='1.0' encoding='utf-8'?>
<widget id="io.arveto.UltimateSnackManagermanager_mobileApp" version="1.0.0">
    <name>ISTY Caf√©teria</name>
    <description>
      A sleek and intuitive UI to manage the cafeterias of the future. The cordova mobile app
    </description>
    <author email="arveto.softwares@gmail.com" href="http://arveto.io">
        The Arveto Developement Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="orientation" value="portrait" />
    <engine name="android" spec="^7.1.1" />
</widget>
